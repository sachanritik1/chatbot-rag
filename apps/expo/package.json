{
  "name": "@chatbot-rag/expo",
  "private": true,
  "main": "index.ts",
  "scripts": {
    "clean": "git clean -xdf .cache .expo .turbo android ios node_modules",
    "dev": "expo start",
    "dev:android": "expo start --android",
    "dev:ios": "expo start --ios",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "format": "prettier --check . --ignore-path ../../.gitignore --ignore-path .prettierignore",
    "lint": "eslint",
    "typecheck": "tsc --noEmit",
    "eas:init": "pnpm dlx eas-cli init",
    "build:preview:android": "pnpm dlx eas-cli build --platform android --profile preview --local",
    "build:preview:ios": "pnpm dlx eas-cli build --platform ios --profile preview --local",
    "build:production:android": "pnpm dlx eas-cli build --platform android --profile production",
    "build:production:ios": "pnpm dlx eas-cli build --platform ios --profile production",
    "android:check-sdk": "node -e \"if(!process.env.ANDROID_HOME){console.error('ANDROID_HOME not set'); process.exit(1)} else {console.log('ANDROID_HOME=', process.env.ANDROID_HOME)}\""
  },
  "dependencies": {
    "@chatbot-rag/config": "workspace:*",
    "@chatbot-rag/shared": "workspace:*",
    "@legendapp/list": "^2.0.14",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@supabase/supabase-js": "^2.87.1",
    "expo": "54.0.29",
    "expo-auth-session": "^7.0.10",
    "expo-clipboard": "^8.0.8",
    "expo-constants": "18.0.12",
    "expo-crypto": "^15.0.8",
    "expo-dev-client": "~6.0.16",
    "expo-linking": "~8.0.8",
    "expo-router": "6.0.19",
    "expo-secure-store": "~15.0.7",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "~3.0.8",
    "expo-system-ui": "~6.0.8",
    "expo-web-browser": "~15.0.8",
    "nativewind": "5.0.0-preview.2",
    "react": "catalog:react19",
    "react-dom": "catalog:react19",
    "react-native": "~0.81.5",
    "react-native-css": "3.0.1",
    "react-native-fetch-api": "^3.0.0",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-markdown-display": "^7.0.2",
    "react-native-reanimated": "~4.1.3",
    "react-native-safe-area-context": "~5.6.1",
    "react-native-screens": "~4.16.0",
    "react-native-url-polyfill": "^3.0.0",
    "react-native-worklets": "~0.5.1",
    "superjson": "2.2.3"
  },
  "devDependencies": {
    "@chatbot-rag/api": "workspace:*",
    "@chatbot-rag/eslint-config": "workspace:*",
    "@chatbot-rag/prettier-config": "workspace:*",
    "@chatbot-rag/tailwind-config": "workspace:*",
    "@chatbot-rag/tsconfig": "workspace:*",
    "@types/react": "catalog:react19",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "tailwindcss": "catalog:",
    "typescript": "catalog:"
  },
  "prettier": "@chatbot-rag/prettier-config"
}
